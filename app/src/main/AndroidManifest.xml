<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chadrick.IMEchanger">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_keyboard_white_48dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_keyboard_white_48dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/appcompat_custom"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".launchwindow"
            android:theme="@style/customstyle"
            >

        </activity>
    </application>

</manifest>